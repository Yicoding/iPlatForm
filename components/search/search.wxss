.container{
    background: #fff;
    height: 100rpx;
    padding: 0 16rpx;
    display: flex;
    align-items: center;
    position: relative;
}

input{
    background: #f5f5f5;
    border-radius: 5rpx;
    padding: 0 20rpx 0 70rpx;
    color: #333;
    flex: 1;
    height: 65rpx;
    border-radius: 33rpx;
    font-size: 26rpx;
}

.icon-search{
    width: 40rpx;
    height: 40rpx;
    position: absolute;
    top: 50%;
    left: 45rpx;
    margin-top: -20rpx;
}

.cancel{
    margin-left: 10rpx;
    color: #34BFA3;
    font-size: 30rpx;
}

.icon-user{
    width: 60rpx;
    height: 60rpx;
    border-radius: 50%;
    margin-left: 10rpx;
}

.btn{
    margin-left: 30rpx;
}

.btn button {
    width: auto !important;
}